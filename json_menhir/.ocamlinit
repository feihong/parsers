(* used by utop *)
open Main;;
open Main.Ast;;

let inputs = [
"true";
"false";
"null";
"1234";
"-56.7";
{| "cool it's a\n \"string\"" |};
{| ["foo", true, 56.8] |};
{| {"foo": 1, "bar": null, "baz": [1, 2, 3]} |};
{|
 [
    1.2,
    true,
    "There's a fly in my \"soup\"",
    {"a": 1,  "b" : 2}
  ]
|};
];;

inputs |> List.iter (fun input -> input |> parse |> show |> print_endline)
